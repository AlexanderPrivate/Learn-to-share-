                </androidx.cardview.widget.CardView>

                <TextView
                    android:id="@+id/text_title1"
                    android:layout_marginTop="18sp"
                    android:text="Tom odel"
                    android:layout_toRightOf="@+id/cardView1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>

                <TextView
                    android:id="@+id/text_descreption1"
                    android:layout_below="@+id/text_title1"
                    android:text="Another love"
                    android:layout_toRightOf="@+id/cardView1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>

                <com.google.android.material.button.MaterialButton
                    android:id="@+id/play_button1"
                    android:layout_alignParentRight="true"
                    android:layout_marginTop="18sp"
                    android:layout_marginRight="8sp"
                    android:text="پخش"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:cornerRadius="20sp" />

                <com.google.android.material.button.MaterialButton
                    android:layout_marginRight="8sp"
                    android:text="دانلود"
                    android:id="@+id/download_button1"
                    android:layout_alignParentRight="true"
                    android:layout_below="@+id/play_button1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:cornerRadius="20sp" />


